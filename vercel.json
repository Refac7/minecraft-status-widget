{
    "rewrites": [{ "source": "/(.*)", "destination": "/index" }],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=60" },
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }